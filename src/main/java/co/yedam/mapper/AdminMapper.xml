<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="co.yedam.mapper.AdminMapper">

 <select id="loginAdmin" parameterType="co.yedam.vo.AdminVO" resultType="co.yedam.vo.AdminVO">
  SELECT * FROM admin 
  WHERE admin_id = #{adminId} 
    AND admin_pw = #{adminPw}
</select>

</mapper>
